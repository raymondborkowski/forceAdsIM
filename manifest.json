{
  "name": "<IM> Incognito Window",
  "description": "Creates an incognito window of current screen with forced ads.",
  "version": "1.2",
  "background": {
    "persistent": true,
    "scripts": ["popup.js"]
  },
  "permissions": [
    "tabs"
],
  "incognito":"split",
  "browser_action": {
    "default_title": "<IM> Incognito Window",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}