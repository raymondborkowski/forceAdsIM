{
  "name": "<IM> Incognito Window",
  "description": "Creates an incognito window of current screen with forced ads.",
  "version": "1.3.3",
  "background": {
    "persistent": true,
    "scripts": ["popup.js"]
  },
  "permissions": [
    "tabs",
    "cookies",
    "*://*.a.intentmedia.net/*"
],
"icons":
   {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
  "browser_action": {
    "default_title": "<IM> Incognito Window",
    "default_icon": "images/icon.png"
  },
  "manifest_version": 2
}